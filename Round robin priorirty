#include<stdio.h>
struct round
{
int p,at,bt,wt,tat,ct,pr,st,f;
}r[50];
float tt=0,awt=0;
main()
{
int i,j,min,b,k,n,c,t,tq,a,rt[50],minp,maxp,g=0;
printf("\nEnter the no. of process ");
scanf("\t%d",&n);
c=n;
for(i=0;i<n;i++)
{
r[i].p=i+1;
r[i].st=2;
printf("\narival time   P%d :\t",i+1);
scanf("\t%d",&r[i].at);
printf(" Burst time  P%d :\t",i+1);
scanf("\t%d",&r[i].bt);
rt[i]=r[i].bt;
printf(" Priority   P%d :\t",i+1);
scanf("\t%d",&r[i].pr);
r[i].f=0;
}
